/*template footbar*/

.footbar {
    position: fixed;
    text-align: center;
    bottom: 0;
    left: 0;
    right: 0;
    background: #fafafa;
    padding: 0 0;
    height: 98rpx;
    z-index: 90;
    display: flex;
    align-items: center;
    justify-content: space-between;
}

.footbar .icon {
    font-size: 44rpx;
    margin: 4rpx 0 1rpx;
}

.footbar .tabbar-item {
    width: 33.333333%;
    font-size: 22rpx;
    color: #777;
    height: 98rpx;
    display: flex;
    align-items: center;
    justify-content: center;
    flex-direction: column;
    transition: 0.4s;
}

.footer-hover {
    color: #ff2828;
    opacity: 0.8;
}

.footbar .active {
    color: #0ec695;
}

.pxview {
    padding-bottom: 62rpx;
}

.button {
    display: block;
    border-radius: 5rpx;
    text-align: center;
    font-size: 32rpx;
    margin: 30rpx 0 0;
    padding:0 0;
    height:78rpx;
    line-height:78rpx;
    background: #0ec695;
    color: #fff;
    border: none;
}

.button-grey {
    background: #eee;
    color: #333;
}

.button-grey-touch {
    background: #ccc;
    color: #333;
}

.button-touch {
    background: #0BB185;
}

.btnbar {
    text-align: center;
    padding: 20px 0 60px;
}

.btnbar .info {
    margin: 0 30rpx 50rpx;
    font-size: 24rpx;
    text-align: left;
    color: #27cfb1;
}

.btnbar .btn {
    display: inline-block;
    width: 40%;
    padding: 26rpx 0;
    font-size: 32rpx;
    background: #0ec695;
    color: #fff;
    margin: 0 5px;
    border-radius: 4rpx;
    vertical-align: top;
    line-height: normal;
}

.btnbar .btn:after {
    border: none;
}

.btnbar .btns-hover {
    background: #ccc;
    color: #888;
}

.button-group {
    position: relative;
}

.button-hover {
    background: #0BB185;
}

.button-red {
    background: #ff2828;
}

.button-disable {
    opacity: 0.5;
}

/* //tabbar */

.tab {
    background: #fff;
    text-align: center;
    padding: 0 0;
    height: 90rpx;
    display: flex;
    align-items: center;
    justify-content: center;
}

.tab>view {
    display: inline-block;
    padding: 0 44rpx;
    border: 1px solid #0ec695;
    margin-left: -1rpx;
    font-size: 28rpx;
    color: #0ec695;
    line-height: 54rpx;
}

.tab>view:first-child {
    border-radius: 4rpx 0 0 4rpx;
}

.tab>view:last-child {
    border-radius: 0 4rpx 4rpx 0;
}

.tab .active {
    background: #0ec695;
    color: #fff;
}

.r-info {
    font-size: 23rpx;
    color: #999;
    display: flex;
    margin: 20rpx 30rpx 0;
    align-items: center;
}

.r-info .ico {
    margin-right: 10rpx;
    color: #999;
    transform: rotate(180deg);
    font-size: 28rpx;
}

.loading-bottom {
    text-align: center;
    opacity: 0.5;
    display: flex;
    align-items: center;
}

.loading-bottom>view {
    display: inline-block;
    width: 14rpx;
    height: 14rpx;
    background: #444;
    border-radius: 90px;
    margin: 0 8rpx;
    position: relative;
    font-size: 0;
    animation: bottomloading ease-in-out 1.4s infinite;
}

.loading-bottom>view:nth-of-type(1) {
    animation-delay: 0s;
}

.loading-bottom>view:nth-of-type(2) {
    animation-delay: 0.4s;
}

.loading-bottom>view:nth-of-type(3) {
    animation-delay: 0.8s;
}

.nomore {
    animation: sliderInUp linear 0.4s;
}

.button-loading {
    transform: scale(0.7);
}

.button-loading>view {
    background: #fff;
}

@keyframes bottomloading {
    0% {
        transform: scale(1);
        opacity: 0.1;
    }

    50% {
        transform: scale(1.3);
        opacity: 1;
    }

    100% {
        transform: scale(1);
        opacity: 0.1;
    }
}

@keyframes sliderUp {
    from {
        transform: translateY(90rpx);
        opacity: 0.2;
    }

    to {
        transform: translateY(0);
        opacity: 1;
    }
}

.f-c-c {
    display: flex;
    align-items: center;
    justify-content: center !important;
}
.f-l-c {
    display: flex;
    align-items: left;
    justify-content: center !important;
}
.pageloading {
    position: fixed;
    left: 0;
    right: 0;
    bottom: 100rpx;
    top: 100rpx;
    display: flex;
    align-items: center;
    justify-content: center;
}

.empty {
    text-align: center;
    position: fixed;
    left: 0;
    top: 0;
    right: 0;
    bottom: 20%;
    display: flex;
    align-items: center;
    flex-direction: column;
    justify-content: center;
    color: #999;
}

.empty .ico, .empty .txt {
    animation: sliderUp 0.4s ease-in both;
}

.empty .txt {
    font-size: 26rpx;
    margin-top: 20rpx;
    animation-delay: 0.2s;
}

.empty .ico {
    font-size: 90rpx;
}

.bline, .lline, .rline,.blines {
    position: relative;
}

.bline:before,.blines:after{
    content: '';
    display: block;
    left: -50%;
    right: -50%;
    bottom: -1px;
    position: absolute;
    border-bottom: 1px solid #eee;
    height: 1px;
    transform: scale(0.5);
}

.lline:after {
    content: '';
    width: 1px;
    top: -50%;
    left: 0;
    bottom: -50%;
    position: absolute;
    border-left: 1px solid #eee;
    transform: scale(0.5);
}

.rline:after {
    content: '';
    width: 3px;
    top: -50%;
    right: -1px;
    bottom: -50%;
    position: absolute;
    border: none !important;
    border-right: 1px solid #eee !important;
    transform: scale(0.5);
}

.typeico {
    font-size: 22rpx;
    width: 80rpx;
    height: 80rpx;
    border: 2px solid #ccc;
    border-radius: 200rpx;
    position: relative;
    display: flex;
    align-items: center;
    justify-content: centerl;
    opacity: 0.7;
    transform: scale(0.9);
}

.typeico text {
    text-align: center;
    border: 1px solid #ccc;
    position: absolute;
    left: -4px;
    right: -4px;
    top: 50%;
    height:44rpx;
    margin-top: -22rpx;
    background: #fafafa;
    border-radius: 3px;
    z-index: 2;
    font-weight: 700;
    color: #ccc;
    transform: rotate(-45deg);
    box-sizing:border-box;
    font-size:25rpx;
    display:flex;
    align-items:center;
    justify-content:center;
}

.typeico:after {
    content: '';
    display: block;
    position: absolute;
    border-radius: 200rpx;
    border: 1px solid #ccc;
    left: 2px;
    top: 2px;
    bottom: 2px;
    right: 2px;
}

.f-c {
    display: flex;
    align-items: center;
}

.f-c-b {
    display: flex;
    align-items: center;
    justify-content: space-between;
}

.flex-c-c {
    position: absolute;
    left: 0;
    right: 0;
    bottom: 0;
    top: 0;
    display: flex;
    align-items: center;
    justify-content: center;
}

.scroll-order {
