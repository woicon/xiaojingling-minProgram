}

.getmore {
    font-size: 26rpx;
    text-align: center;
    line-height: 2;
    padding: 40rpx 0;
    color: #ccc;
}

.o-search {
    background: #fff;
    align-items: center;
    padding: 0 40rpx;
    transition: 0.5s;
    z-index: 10;
    height: 90rpx;
    justify-content: space-between;
    position: absolute;
    top: 110rpx;
    left: 0;
    right: 0;
    display: none;
}

.o-search-show {
    display: flex;
}

.mask {
    position: fixed;
    left: 0;
    right: 0;
    bottom: 0;
    top: 0;
    background: rgba(0, 0, 0, 0.6);
    display: none;
}

.mask.show {
    display: block;
}

.o-search .ico {
    color: #666;
}

.o-search input {
    padding: 18rpx 0 18rpx 20rpx;
    font-size: 29rpx;
    color: #444;
    transition: 0;
}

.o-input {
    flex-grow: 1;
    overflow: hidden;
}

.search-del {
    width: 90rpx;
    font-size: 28rpx;
    text-align: right;
    color: #888;
    z-index: 2;
    display: flex;
    align-items: center;
    justify-content: center;
    background: #fff;
    height: 90rpx;
    margin-right: -40rpx;
}

.search-del-touch {
    opacity: 0.3;
}

.nav-touch, .view-touch {
    background: #fff;
    opacity: 0.8;
}

.o-summary {
    background:#0ec695;
    color: rgba(255, 255, 255, 0.7);
    display: flex;
    font-size: 23rpx;
    line-height: 1.4;
    align-items: center;
    justify-content: space-between;
    text-align: center;
    height: 110rpx;
}

.os-value {
    color: #fff;
    font-size: 28rpx;
}

.o-summary>view {
    width: 33.33333333%;
}

.o-summary>view::after {
