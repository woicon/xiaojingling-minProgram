.heads{
    background:#0ec695;
    color:#fff;
    padding:40rpx;
}
.price{
    font-weight:800;
    font-size:90rpx;
}
.heads{
    display:flex;
    align-items:center;
    justify-content:space-between;
}
.outbtn{
    font-size:32rpx;
    padding:10rpx 40rpx;
    border:1px solid #fff;
    border-radius:3rpx;

}

.h-title{
    font-size:26rpx;
}

.list>view{ 
    background:#fff;
    padding:28rpx 40rpx;
    font-size:30rpx;
    display:flex;
    align-items:center;
    justify-content:space-between;
    position:relative;
    color:#444;
}

.list>view::after{
    content:'';
    display:block;
    width:14rpx;
    height:14rpx;
    border:2px solid #ccc;
    border-left:none;
    border-bottom:none;
    transform: rotate(45deg);
}

.timebar{
    display:flex;
    align-items:center;
    justify-content:space-between;
}